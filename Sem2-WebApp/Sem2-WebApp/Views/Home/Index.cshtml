                    @if (User.Identity.IsAuthenticated)
                    {
                        <span class="intro-heading" style="color: #ffffff">Currently signed in as <b>@User.FindFirst("name").Value</b></span>
                        <div class="header-buttons">
                            <a asp-area="AzureAD" asp-controller="Account" asp-action="SignOut" class="primary-button">SIGN OUT</a>
                            <a href="/AdminHome" class="secondary-button">CONSOLE</a>
                        </div>
                    }
                    else
                    {
                        <span class="intro-heading" style="color: #ffffff">Spend less time worrying about toilets and more focusing on productivity</span>
                        <div class="header-buttons">
                            <a asp-area="AzureAD" asp-controller="Account" asp-action="SignIn" class="secondary-button">SIGN IN</a>
                        </div>
                    }
